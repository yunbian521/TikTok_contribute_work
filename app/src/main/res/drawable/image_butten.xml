<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 核心：设置圆角（四个角统一/单独设置） -->
    <corners
        android:radius="20dp"/>

    <!-- 可选：按钮背景色（透明/纯色，推荐透明避免遮挡图片）
    <solid android:color="@android:color/transparent" />-->

    <!-- 可选：添加边框（如需）
    <stroke
        android:width="1dp"
        android:color="#EEEEEE"
    /> -->
</shape>